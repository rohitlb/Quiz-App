{
  "name": "category",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "description": {
      "type": "string",
      "required": true
    },
    "userid": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userid",
      "options": {
        "nestRemoting": true
      }
    },
    "quizzes": {
      "type": "hasMany",
      "model": "quiz",
      "foreignKey": "categoryid",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
