{
  "name": "quiz",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "startdate": {
      "type": "date",
      "required": true
    },
    "enddate": {
      "type": "date",
      "required": true
    },
    "categoryid": {
      "type": "number",
      "required": true
    },
    "adminid": {
      "type": "number",
      "required": true
    },
    "studentid": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "admin": {
      "type": "belongsTo",
      "model": "admin",
      "foreignKey": "adminid",
      "options": {
        "nestRemoting": true
      }
    },
    "category": {
      "type": "belongsTo",
      "model": "category",
      "foreignKey": "categoryid",
      "options": {
        "nestRemoting": true
      }
    },
    "student": {
      "type": "belongsTo",
      "model": "student",
      "foreignKey": "studentid",
      "options": {
        "nestRemoting": true
      }
    },
    "questions": {
      "type": "hasMany",
      "model": "question",
      "foreignKey": "quizid",
      "options": {
        "nestRemoting": true
      }
    },
    "results": {
      "type": "hasOne",
      "model": "result",
      "foreignKey": "quizid",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
